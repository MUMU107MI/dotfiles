[core]
	autocrlf = input
##	editor = "code --wait "

[color]
	ui = true

[user]
	name = Furukawa, Atsushi
	email = atsushifx@gmail.com

[difftool "sourcetree"]
	cmd = '' \"$LOCAL\" \"$REMOTE\"

[mergetool "sourcetree"]
	cmd = "'' "
	trustExitCode = true

[push]
	default = current

[init]
	defaultBranch = main

[alias]
	co = checkout
	st = status
	fixit = commit --amend --no-edit
	graph = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit

[credential]
	helper = manager-core

[credential "helperselector"]
	selected = store

[ghq]
	root = c:/Users/atsushifx/workspaces/develop
	root = c:/Users/atsushifx/workspaces/education
	root = c:/Users/atsushifx/workspaces/gitworks

[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
